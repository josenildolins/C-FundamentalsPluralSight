package com.pluralsight;

import org.openqa.selenium.By;

public class PathPages {

	static String url = "https://www.pluralsight.com/paths";
	static String title = "Java | Pluralsight";
	
	public void goTo() {
		Browser.goTo(url);
		
	}
	public boolean isAt() {
		return Browser.title().equals(title);
	}

	
	

}
